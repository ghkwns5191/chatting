<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="chatting">


<select id="getDialogs" resultType="map">
/* chatting.getDialogs : 채팅목록 조회 */
SELECT
	REG_USR
	, MESSAGE
	, REG_DATE
FROM DIALOG
</select>




















</mapper>